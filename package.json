{
  "name": "nios-ii",
  "displayName": "Nios II",
  "description": "Nios II Language Support for Nois II Assembly Language Programming",
  "publisher": "niosii",
  "version": "0.0.8",
  "repository": "https://github.com/estKey/nios-ii",
  "engines": {
    "vscode": "^1.36.0"
  },
  "scripts": {
    "build": "rimraf *.vsix && vsce package"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "vsce": "1.66.0",
    "vscode": "1.1.35"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "niosii",
        "aliases": [
          "Nios II",
          "niosii",
          "niosiiasm",
          "niosii asm"
        ],
        "extensions": [
          ".s",
          ".niosii",
          ".eds",
          ".elf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "niosii",
        "path": "./snippets/niosiiasm.json"
      },
      {
        "language": "niosii",
        "path": "./snippets/niosiisudo.json"
      }
    ],
    "grammars": [
      {
        "language": "niosii",
        "scopeName": "source.s",
        "path": "./syntaxes/niosii.assembly.tmLanguage.json"
      }
    ]
  }
}
